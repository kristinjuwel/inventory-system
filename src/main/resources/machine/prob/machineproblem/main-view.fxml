<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="540.0" prefWidth="963.0" stylesheets="@main.css" xmlns="http://javafx.com/javafx/19"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="machine.prob.machineproblem.Controller">
   <ImageView fitHeight="540.0" fitWidth="963.0" pickOnBounds="true" preserveRatio="true">
      <Image url="@HOME PANE.png"/>
   </ImageView>
   <Pane layoutX="17.0" layoutY="131.0" prefHeight="375.0" prefWidth="176.0">
      <Button fx:id="home_p" mnemonicParsing="false" onAction="#home" styleClass="side_buttons" text="HOME"/>
      <Button fx:id="menu_p" layoutY="68.0" mnemonicParsing="false" onAction="#menu" styleClass="side_buttons"
              text="MENU"/>
      <Button fx:id="stock_p" layoutY="137.0" mnemonicParsing="false" onAction="#stocks" styleClass="side_buttons"
              text="STOCKS"/>
      <Button fx:id="report_p" layoutY="267.0" mnemonicParsing="false" onAction="#report" styleClass="side_buttons"
              text="REPORT"/>
      <Button fx:id="quit" layoutY="334.0" mnemonicParsing="false" onAction="#exit" prefHeight="25.0"
              prefWidth="126.0" styleClass="side_buttons" text="EXIT">
      </Button>
   </Pane>
</AnchorPane>
